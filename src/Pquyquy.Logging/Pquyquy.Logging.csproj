<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<LangVersion>latest</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<OutputType>Library</OutputType>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<AssemblyName>Pquyquy.Logging</AssemblyName>
		<AssemblyTitle>Pquyquy.Logging Library</AssemblyTitle>
		<Product>Pquyquy.Logging Library</Product>
		<RootNamespace>Pquyquy.Logging</RootNamespace>
		<Authors>Andres Martinez</Authors>
		<Copyright>Copyright Â© 2024</Copyright>
		<Description>Logging infrastructure implementation with support for multiple libraries like log4net, Serilog, and NLog.</Description>
		<Summary>A flexible and extensible logging library for .NET applications.</Summary>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Version>1.0.0</Version>
		<PackageTags>logging;log4net;serilog;nlog</PackageTags>
		<PackageProjectUrl>https://github.com/</PackageProjectUrl>
		<RepositoryUrl>https://github.com/</RepositoryUrl>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="log4net" Version="2.0.17" />
	</ItemGroup>

</Project>
